generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Referral {
  id             Int      @id @default(autoincrement())
  referrerName   String
  referrerEmail  String
  friendName     String
  friendEmail    String
  course         String
  message        String?
  createdAt      DateTime @default(now())
}
